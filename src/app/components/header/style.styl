.header
  padding 18px 20px 10px 20px
  background-color #212121
  width 100%
  height 160px

li
  display inline 
  padding-left 22px

ul
  padding-left 0px
  display inline
  position: relative;
  top: -12px;

.title
  display inline-block
  position relative
  top 36px
  color #bbbbbb
  font-size 35px

.header-content
  height max-content
  width 100%

.github-icon  
  position relative
  top 14px
  display inline-block
  width 35px
  height 35px
  background-image url('./img/github.svg')

.header-actions
  display inline-block
  width calc(100% - 38px)
  text-align right